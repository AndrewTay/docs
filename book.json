{
  "gitbook": "3.2.2",
  "title": "Workato Docs",
  "plugins": [
    "edit-link",
    "-sharing",
    "-fontsettings",
    "favicon",
    "toggle-chapters",
    "intopic-toc",
    "ga"
  ],
  "variables": {
    "shareablePages": [
      "oem/embedded-connections.md",
      "oem/jwt-direct-linking.md"
    ]
  },
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/workato/docs/edit/master/",
      "label": "",
      "multilingual": false
    },
    "ga": {
      "token": "UA-104783574-1"
    },
    "intopic-toc": {
      "label": "Contents",
      "visible": false
    },
    "favicon": {
      "shortcut": "/assets/images/favicon/favicon.ico",
      "bookmark": "/assets/images/favicon/favicon.ico",
      "appleTouch": "/assets/images/favicon/android-chrome-36x36.png"
    }
  }
}
